<p style="color: green"><%= notice %></p>

<h1>Tasks</h1>

<div>
  <%= p session.to_h %>
</div>

<div>
  <%= p current_account.attributes %>
</div>

<div id="tasks">
  <% @tasks.each do |task| %>
    <%= render task %>
    <p>
      <%= link_to "Show this task", task %>
    </p>
  <% end %>
</div>

<%= link_to "New task", new_task_path %>
